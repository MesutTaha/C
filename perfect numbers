#include <stdio.h>
#include <stdlib.h>

void PerfectNumbers();
int main() {
	
	PerfectNumbers();
	return 0;
}
void PerfectNumbers() {
	int i, j, sum;
	for (i = 1; i < 10000; i++) {
		sum = 0;
		for (j = 1; j < i; j++) {
			if(i%j==0){
				sum = sum + j;
			}
		}
		if(i==sum){
			printf("%d\n", sum);
		}
		}
	}
	

	
	

