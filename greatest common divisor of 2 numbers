#include <stdio.h>
#include <stdlib.h>
#define scanf scanf_s
void GCD();

int main()
{
	GCD();
	return 0;
}

void GCD()
{
	int num1, num2, i, j, k;
	printf("Enter two integers: \n");
	scanf("%d \n %d", &num1, &num2);

	if (num1 > num2)
		i = num1;
	else
		i = num2;

	for (j = 1; j <= i; j++)
	{
		if (num1 % j == 0 && num2 % j == 0) {
			k = j;
		}
			
	}

	printf("Greatest Common Divisor=%d", k);

}
